{
    "name": "libmodulor-examples-supertrader",
    "version": "0.1.0",
    "author": "Chafik H'nini <chafik.hnini@gmail.com>",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "./build.sh",
        "build:analyze:web": "vite-bundle-visualizer -c ./src/products/SuperTrader/web/vite.config.ts",
        "clean": "./clean.sh",
        "deploy:server-cloudflare-worker": "cd dist/products/SuperTrader/server-cloudflare-worker && wrangler deploy",
        "run:cli": "cd dist/products/SuperTrader/cli && node index.js",
        "run:rn:android": "cd dist/products/SuperTrader/rn && expo start --android",
        "run:rn:ios": "cd dist/products/SuperTrader/rn && expo start --ios",
        "run:server-cloudflare-worker": "cd dist/products/SuperTrader/server-cloudflare-worker && wrangler telemetry disable && wrangler dev",
        "run:server-nextjs": "cd dist/products/SuperTrader/server-nextjs && next dev",
        "run:server-node-express": "cd dist/products/SuperTrader/server-node-express && node --env-file .env index.js",
        "run:server-node-hono": "cd dist/products/SuperTrader/server-node-hono && node --env-file .env index.js",
        "test": "tsc && vitest run --passWithNoTests"
    },
    "dependencies": {
        "@hono/node-server": "^1.19.1",
        "@modelcontextprotocol/sdk": "^1.17.5",
        "@tailwindcss/vite": "^4.1.13",
        "daisyui": "^5.1.8",
        "expo": "^53.0.22",
        "hono": "^4.9.6",
        "inversify": "^7.9.1",
        "knex": "^3.1.0",
        "libmodulor": "^0.21.0",
        "next": "^15.5.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-native": "^0.79.6",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "tailwindcss": "^4.1.13"
    },
    "devDependencies": {
        "@babel/plugin-transform-class-static-block": "^7.28.3",
        "@tailwindcss/postcss": "^4.1.13",
        "@types/node": "^22.18.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitest/coverage-v8": "^3.2.4",
        "babel-plugin-parameter-decorator": "^1.0.16",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "buffer": "^6.0.3",
        "cookie-parser": "^1.4.7",
        "express": "^5.1.0",
        "express-fileupload": "^1.5.2",
        "fast-check": "^4.3.0",
        "helmet": "^8.1.0",
        "jose": "^6.1.0",
        "typescript": "^5.9.2",
        "vite": "^6.3.6",
        "vite-bundle-visualizer": "^1.2.1",
        "vitest": "^3.2.4",
        "wrangler": "^4.34.0"
    }
}
