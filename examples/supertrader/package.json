{
    "name": "libmodulor-examples-supertrader",
    "version": "0.1.0",
    "author": "Chafik H'nini <chafik.hnini@gmail.com>",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "./build.sh",
        "build:analyze:web": "vite-bundle-visualizer -c ./src/products/SuperTrader/web/vite.config.ts",
        "run:cli": "cd dist/products/SuperTrader/cli && node index.js",
        "run:rn:android": "cd dist/products/SuperTrader/rn && expo start --android",
        "run:rn:ios": "cd dist/products/SuperTrader/rn && expo start --ios",
        "run:server-nextjs": "cd src/products/SuperTrader/server-nextjs && next dev",
        "run:server-node-express": "cd dist/products/SuperTrader/server-node-express && node --env-file .env index.js",
        "run:server-node-hono": "cd dist/products/SuperTrader/server-node-hono && node --env-file .env index.js",
        "test": "tsc && vitest run --passWithNoTests"
    },
    "dependencies": {
        "@hono/node-server": "^1.14.3",
        "@modelcontextprotocol/sdk": "^1.12.0",
        "@tailwindcss/vite": "^4.1.7",
        "daisyui": "^5.0.37",
        "expo": "^53.0.9",
        "hono": "^4.7.10",
        "inversify": "^7.5.1",
        "knex": "^3.1.0",
        "libmodulor": "^0.16.0",
        "next": "^15.3.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-native": "^0.79.2",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "tailwindcss": "^4.0.12"
    },
    "devDependencies": {
        "@babel/plugin-transform-class-static-block": "^7.27.1",
        "@tailwindcss/postcss": "^4.1.7",
        "@types/node": "^22.15.21",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@vitest/coverage-v8": "^3.1.4",
        "babel-plugin-parameter-decorator": "^1.0.16",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "buffer": "^6.0.3",
        "cookie-parser": "^1.4.7",
        "express": "^5.1.0",
        "express-fileupload": "^1.5.1",
        "fast-check": "^4.1.1",
        "helmet": "^8.1.0",
        "jose": "^6.0.11",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-bundle-visualizer": "^1.2.1",
        "vitest": "^3.1.4"
    }
}
